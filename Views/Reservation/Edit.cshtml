@model Reservation

@{
    ViewData["Title"] = "Edytuj rezerwacje";
    <form asp-action="EditR" asp-controller="Reservation" method="post">
            <h2 class="mb-4">Rezerwacja</h2>
            <input asp-for="RoomId" value="@Model.RoomId" type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" hidden>
            <input asp-for="Id" value="@Model.Id" type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default" hidden>
            <div class="input-group mb-3">
                <span class="input-group-text" id="inputGroup-sizing-default">Zameldowanie</span>
                <input asp-for="begin" type="datetime-local" value="@Model.begin" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
            </div>
            <div class="input-group mb-3">
                <span class="input-group-text" id="inputGroup-sizing-default">Wymeldowanie</span>
            <input asp-for="end" type="datetime-local" value="@Model.end" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
            </div>
            <button type="submit" class="btn btn-secondary">Edytuj rezerwacje</button>
    </form>
}
